<VirtualHost *:80>
   ServerName www.ugwulo.me
   ServerAlias ugwulo.me
   DocumentRoot /var/www/html/{{vhost_name}}/public
   <Directory /var/www/html/{{vhost_name}}>
       AllowOverride All
   </Directory>
   ErrorLog ${APACHE_LOG_DIR}/error.log
   CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>